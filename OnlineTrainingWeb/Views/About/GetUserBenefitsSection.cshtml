@model  ViewModel.ListOfViewModels

@if (Model.ListofUserBenefitsSection !=null)
{
    <div class="container">
        <section class="p-md-3 mx-md-5 text-lg-left">
            <div class="row text-center d-flex justify-content-center">
                @foreach (var item in Model.ListofUserBenefitsSection)
                {
                    <div class="col-lg-3 col-md-4 mb-md-0 mb-5 HomebannerExplorationTextColor">
                        <img src="@item.IconUrl" class="img-fluid" />
                        <h6 class=" mb-4">@item.MainTitle</h6>

                    </div>
                }

            </div>
        </section>
    </div>
}
